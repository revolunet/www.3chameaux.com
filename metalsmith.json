{
  "metadata": {
    "title": "3chameaux.com",
    "description": "MetalSmith powered" 
  },
  "plugins": {
    "metalsmith-paths": {},
    "metalsmith-imagemin": {
        "optimizationLevel": 3
    },
    "metalsmith-register-helpers": {},
    "metalsmith-i18n": {
      "default": "fr",
      "locales": ["en", "fr"],
      "directory": "locales"
    },
    "metalsmith-in-place": {
      "engine": "handlebars",
      "partials": "partials"
    },
    "metalsmith-markdown": {
      "breaks": false,
      "smartLists": false,
      "gfm": true
    },
    "metalsmith-sass": {
      "sourceMap": true,
      "outputStyle": "compressed",
      "sourceMapContents": true
    },
    "metalsmith-layouts": {
      "engine": "handlebars"
    },
    "metalsmith-concat": {
      "files": [
        "assets/js/jquery.min.js",
        "assets/js/jquery.dropotron.min.js",
        "assets/js/jquery.scrollex.min.js",
        "assets/js/skel.min.js",
        "assets/js/util.js",
        "assets/js/ie/respond.min.js",
        "assets/js/main.js"
      ],
      "output": "assets/js/prod.js"
    },
    "metalsmith-uglify": {}
  }
}